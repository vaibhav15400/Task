lect\n              InputLabelProps={{ id: 'scroll-playground-overflow-label' }}\n              SelectProps={{\n                native: true,\n                inputProps: { 'aria-labelledby': 'scroll-playground-overflow-label' },\n              }}\n              value={preventOverflow}\n              onChange={(event) => {\n                setPreventOverflow(event.target.value);\n              }}\n            >\n              <option value=\"disabled\">disabled</option>\n              <option value=\"scrollParent\">scrollParent</option>\n              <option value=\"viewport\">viewport</option>\n              <option value=\"window\">window</option>\n            </TextField>\n          </div>\n          <FormControlLabel\n            control={\n              <Switch\n                checked={flip}\n                onChange={(event) => {\n                  setFlip(event.target.checked);\n                }}\n                value=\"flip\"\n              />\n            }\n            label={[\n              'Flip',\n              '(flip the popper\u2019s placement when it starts to overlap its reference element)',\n            ].join(' ')}\n          />\n          <FormControlLabel\n            control={\n              <Switch\n                checked={arrow}\n                onChange={(event) => {\n                  setArrow(event.target.checked);\n                }}\n                value=\"arrow\"\n              />\n            }\n            label=\"Arrow\"\n          />\n        </Grid>\n      </Grid>\n      <HighlightedCode code={jsx} language=\"jsx\" />\n    </div>\n  );\n}\n"},lDfW:function(e,n){e.exports='---\ntitle: React Popper\uff08\u5f39\u51fa\u63d0\u793a\uff09\u7ec4\u4ef6\ncomponents: Popper\n---\n\n# Popper \u5f39\u51fa\u63d0\u793a\n\n<p class="description">\u4f7f\u7528\u5f39\u51fa\u63d0\u793a\u7ec4\u4ef6\uff0c\u60a8\u53ef\u5728\u53e6\u4e00\u4e2a\u5143\u7d20\u4e4b\u4e0a\u663e\u793a\u4e00\u4e9b\u5185\u5bb9\u3002 \u8fd9\u53ef\u4ee5\u66ff\u4ee3 react-popp